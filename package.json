{
  "name": "semantic-color-grep-edit",
  "displayName": "Semantic Color Grep Edit",
  "description": "A VS Code extension for searching with ripgrep and displaying colorized results with file links",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:semantic-color-grep-edit.search"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "semantic-color-grep-edit.search",
        "title": "Semantic Color Grep Edit: Search"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/vscode": "^1.60.0",
    "@types/node": "16.x",
    "typescript": "^4.9.4"
  }
}